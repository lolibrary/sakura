{
    "name": "lolibrary/sakura",
    "type": "project",
    "description": "Lolibrary's web frontend",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "dillingham/nova-attach-many": "^1.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.18",
        "laravel/nova": "^2.3",
        "laravel/passport": "^7.5",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "lolibrary/image-array": "*",
        "sentry/sentry-laravel": "^1.6",
        "whitecube/nova-flexible-content": "^0.1.11",
        "stevebauman/purify": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "facade/ignition": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/ImageArray"
        }
    ]
}
